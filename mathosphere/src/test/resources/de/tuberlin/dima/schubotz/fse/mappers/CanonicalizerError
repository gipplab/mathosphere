<math xmlns="http://www.w3.org/1998/Math/MathML" xref="S2.SS3.p5.5.m3.1.cmml" alttext="f_{\varepsilon}\in{\cal C}[0,T]" class="ltx_Math" id="S2.SS3.p5.5.m3.1" display="inline"><semantics xref="S2.SS3.p5.5.m3.1.cmml" id="S2.SS3.p5.5.m3.1a"><mrow xref="S2.SS3.p5.5.m3.1.10.cmml" id="S2.SS3.p5.5.m3.1.10"><msub xref="S2.SS3.p5.5.m3.1.10.1.cmml" id="S2.SS3.p5.5.m3.1.10.1"><mi xref="S2.SS3.p5.5.m3.1.1.cmml" id="S2.SS3.p5.5.m3.1.1">f</mi><mi xref="S2.SS3.p5.5.m3.1.2.1.cmml" id="S2.SS3.p5.5.m3.1.2.1">Œµ</mi></msub><mo xref="S2.SS3.p5.5.m3.1.3.cmml" id="S2.SS3.p5.5.m3.1.3">‚àà</mo><mrow xref="S2.SS3.p5.5.m3.1.10.2.cmml" id="S2.SS3.p5.5.m3.1.10.2"><mi xref="S2.SS3.p5.5.m3.1.4.cmml" id="S2.SS3.p5.5.m3.1.4">ùíû</mi><mo xref="S2.SS3.p5.5.m3.1.10.2.1.cmml" id="S2.SS3.p5.5.m3.1.10.2.1">‚Å¢</mo><mrow xref="S2.SS3.p5.5.m3.1.10.2.2.cmml" id="S2.SS3.p5.5.m3.1.10.2.2"><mo xref="S2.SS3.p5.5.m3.1.10.2.2.cmml" id="S2.SS3.p5.5.m3.1.10.2.2a">[</mo><mrow xref="S2.SS3.p5.5.m3.1.10.2.2.cmml" id="S2.SS3.p5.5.m3.1.10.2.2b"><mn xref="S2.SS3.p5.5.m3.1.6.cmml" id="S2.SS3.p5.5.m3.1.6">0</mn><mo xref="S2.SS3.p5.5.m3.1.10.2.2.cmml" id="S2.SS3.p5.5.m3.1.10.2.2c">,</mo><mi xref="S2.SS3.p5.5.m3.1.8.cmml" id="S2.SS3.p5.5.m3.1.8">T</mi></mrow><mo xref="S2.SS3.p5.5.m3.1.10.2.2.cmml" id="S2.SS3.p5.5.m3.1.10.2.2d">]</mo></mrow></mrow></mrow><annotation-xml xref="S2.SS3.p5.5.m3.1" id="S2.SS3.p5.5.m3.1.cmml" encoding="MathML-Content"><apply xref="S2.SS3.p5.5.m3.1.10" id="S2.SS3.p5.5.m3.1.10.cmml"><in xref="S2.SS3.p5.5.m3.1.3" id="S2.SS3.p5.5.m3.1.3.cmml"></in><apply xref="S2.SS3.p5.5.m3.1.10.1" id="S2.SS3.p5.5.m3.1.10.1.cmml"><csymbol cd="ambiguous" id="S2.SS3.p5.5.m3.1.10.1.1.cmml">subscript</csymbol><ci xref="S2.SS3.p5.5.m3.1.1" id="S2.SS3.p5.5.m3.1.1.cmml">f</ci><ci xref="S2.SS3.p5.5.m3.1.2.1" id="S2.SS3.p5.5.m3.1.2.1.cmml">Œµ</ci></apply><apply xref="S2.SS3.p5.5.m3.1.10.2" id="S2.SS3.p5.5.m3.1.10.2.cmml"><times xref="S2.SS3.p5.5.m3.1.10.2.1" id="S2.SS3.p5.5.m3.1.10.2.1.cmml"></times><ci xref="S2.SS3.p5.5.m3