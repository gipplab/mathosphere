and
ùíØ‚Å¢(L)‚à©EP‚Å¢(L)tor‚â†‚àÖùíØLsubscriptEPsubscriptLtor{\mathcal{T}}(L)\cap E_{P}(L)_{\text{tor}}\not=\emptyset</mtext></apply></apply><apply xref="S2.E2.m1.1.24.3" id="S2.E2.m1.1.24.3.cmml"><times xref="S2.E2.m1.1.24.3.1" id="S2.E2.m1.1.24.3.1.cmml"></times><apply xref="S2.E2.m1.1.24.3.2" id="S2.E2.m1.1.24.3.2.cmml"><csymbol cd="ambiguous" id="S2.E2.m1.1.24.3.2.1.cmml">subscript</csymbol><ci xref="S2.E2.m1.1.17" id="S2.E2.m1.1.17.cmml">N</ci><apply xref="S2.E2.m1.1.18.1" id="S2.E2.m1.1.18.1.cmml"><list id="S2.E2.m1.1.18.1.4.cmml"></list><ci xref="S2.E2.m1.1.18.1.1" id="S2.E2.m1.1.18.1.1.cmml">B</ci><ci xref="S2.E2.m1.1.18.1.3" id="S2.E2.m1.1.18.1.3.cmml">L</ci></apply></apply><apply xref="S2.E2.m1.1.24.3.3" id="S2.E2.m1.1.24.3.3.cmml"><csymbol cd="ambiguous" id="S2.E2.m1.1.24.3.3.1.cmml">superscript</csymbol><ci xref="S2.E2.m1.1.20" id="S2.E2.m1.1.20.cmml">x</ci><apply xref="S2.E2.m1.1.22.1" id="S2.E2.m1.1.22.1.cmml"><csymbol cd="ambiguous" id="S2.E2.m1.1.22.1.3.cmml">subscript</csymbol><ci xref="S2.E2.m1.1.22.1.1" id="S2.E2.m1.1.22.1.1.cmml">Œª</ci><apply xref="S2.E2.m1.1.22.1.2.1" id="S2.E2.m1.1.22.1.2.1.cmml"><list id="S2.E2.m1.1.22.1.2.1.4.cmml"></list><ci xref="S2.E2.m1.1.22.1.2.1.1" id="S2.E2.m1.1.22.1.2.1.1.cmml">E</ci><ci xref="S2.E2.m1.1.22.1.2.1.3" id="S2.E2.m1.1.22.1.2.1.3.cmml">L</ci></apply></apply></apply></apply></apply></annotation-xml><annotation xref="S2.E2.m1.1.cmml" id="S2.E2.m1.1b" encoding="application/x-tex">\#\{P\in B(L)-\Delta_{\pi}:\text{$H_{B,L}(P)&lt;x$
and
${\mathcal{T}}(L)\cap E_{P}(L)_{\text{tor}}\not=\emptyset$}\}\gg_{E}N_{B,L}(x)%
