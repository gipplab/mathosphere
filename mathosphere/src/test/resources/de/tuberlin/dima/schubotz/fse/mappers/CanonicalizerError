<p class="ltx_p" id="id77.7">where <math xmlns="http://www.w3.org/1998/Math/MathML" xref="id77.7.1.1.cmml" alttext="(f_{\nu,j,k})_{k\in{\tenmsb N}}" class="ltx_Math" id="id77.7.1.1" display="inline"><semantics xref="id77.7.1.1.cmml" id="id77.7.1.1a"><msub xref="id77.7.1.1.6.cmml" id="id77.7.1.1.6"><mrow xref="id77.7.1.1.6.2.cmml" id="id77.7.1.1.6.2"><mo xref="id77.7.1.1.6.2.cmml" id="id77.7.1.1.6.2a">(</mo><msub xref="id77.7.1.1.6.2.cmml" id="id77.7.1.1.6.2b"><mi xref="id77.7.1.1.2.cmml" id="id77.7.1.1.2">f</mi><mrow xref="id77.7.1.1.3.1.cmml" id="id77.7.1.1.3.1"><mi xref="id77.7.1.1.3.1.1.cmml" id="id77.7.1.1.3.1.1">ŒΩ</mi><mo xref="id77.7.1.1.3.1.cmml" id="id77.7.1.1.3.1a">,</mo><mi xref="id77.7.1.1.3.1.3.cmml" id="id77.7.1.1.3.1.3">j</mi><mo xref="id77.7.1.1.3.1.cmml" id="id77.7.1.1.3.1b">,</mo><mi xref="id77.7.1.1.3.1.5.cmml" id="id77.7.1.1.3.1.5">k</mi></mrow></msub><mo xref="id77.7.1.1.6.2.cmml" id="id77.7.1.1.6.2c">)</mo></mrow><mrow xref="id77.7.1.1.5.1.cmml" id="id77.7.1.1.5.1"><mi xref="id77.7.1.1.5.1.1.cmml" id="id77.7.1.1.5.1.1">k</mi><mo xref="id77.7.1.1.5.1.2.cmml" id="id77.7.1.1.5.1.2">‚àà</mo><mi xref="id77.7.1.1.5.1.3.cmml" mathvariant="normal" id="id77.7.1.1.5.1.3">‚Ñï</mi></mrow></msub><annotation-xml xref="id77.7.1.1" id="id77.7.1.1.cmml" encoding="MathML-Content"><apply xref="id77.7.1.1.6" id="id77.7.1.1.6.cmml"><csymbol cd="ambiguous" id="id77.7.1.1.6.1.cmml">subscript</csymbol><apply xref="id77.7.1.1.6.2" id="id77.7.1.1.6.2.cmml"><csymbol cd="ambiguous" id="id77.7.1.1.6.2.1.cmml">subscript</csymbol><ci xref="id77.7.1.1.2" id="id77.7.1.1.2.cmml">f</ci><apply xref="id77.7.1.1.3.1" id="id77.7.1.1.3.1.cmml"><list id="id77.7.1.1.3.1.6.cmml"></list><ci xref="id77.7.1.1.3.1.1" id="id77.7.1.1.3.1.1.cmml">ŒΩ</ci><ci xref="id77.7.1.1.3.1.3" id="id77.7.1.1.3.1.3.cmml">j</ci><ci xref="id77.7.1.1.3.1.5" id="id77.7.1.1.3.1.5.cmml">k</ci></apply></apply><apply xref="id77.7.1.1.5.1" id="id77.7.1.1.5.1.cmml"><in xref="id77.7.1.1.5.1.2" id="id77.7.1.1.5.1.2.cmm