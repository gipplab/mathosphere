<p class="ltx_p" id="S3.p1.16">where <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.16.m1.1.cmml" alttext="A" class="ltx_Math" id="S3.p1.16.m1.1" display="inline"><semantics xref="S3.p1.16.m1.1.cmml" id="S3.p1.16.m1.1a"><mi xref="S3.p1.16.m1.1.1.cmml" id="S3.p1.16.m1.1.1">A</mi><annotation-xml xref="S3.p1.16.m1.1" id="S3.p1.16.m1.1.cmml" encoding="MathML-Content"><ci xref="S3.p1.16.m1.1.1" id="S3.p1.16.m1.1.1.cmml">A</ci></annotation-xml><annotation xref="S3.p1.16.m1.1.cmml" id="S3.p1.16.m1.1b" encoding="application/x-tex">A</annotation></semantics></math> is such that <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.16.m2.1.cmml" alttext="\int_{0}^{A}\phi(s)ds=f-1/\lambda^{p-1}" class="ltx_Math" id="S3.p1.16.m2.1" display="inline"><semantics xref="S3.p1.16.m2.1.cmml" id="S3.p1.16.m2.1a"><mrow xref="S3.p1.16.m2.1.17.cmml" id="S3.p1.16.m2.1.17"><mrow xref="S3.p1.16.m2.1.17.1.cmml" id="S3.p1.16.m2.1.17.1"><msubsup xref="S3.p1.16.m2.1.17.1.1.cmml" id="S3.p1.16.m2.1.17.1.1"><mo xref="S3.p1.16.m2.1.1.cmml" id="S3.p1.16.m2.1.1">‚à´</mo><mn xref="S3.p1.16.m2.1.2.1.cmml" id="S3.p1.16.m2.1.2.1">0</mn><mi xref="S3.p1.16.m2.1.3.1.cmml" id="S3.p1.16.m2.1.3.1">A</mi></msubsup><mrow xref="S3.p1.16.m2.1.17.1.2.cmml" id="S3.p1.16.m2.1.17.1.2"><mi xref="S3.p1.16.m2.1.4.cmml" id="S3.p1.16.m2.1.4">œï</mi><mo xref="S3.p1.16.m2.1.17.1.2.1.cmml" id="S3.p1.16.m2.1.17.1.2.1">‚Å¢</mo><mrow xref="S3.p1.16.m2.1.6.cmml" id="S3.p1.16.m2.1.6"><mo xref="S3.p1.16.m2.1.6.cmml" id="S3.p1.16.m2.1.6a">(</mo><mi xref="S3.p1.16.m2.1.6.cmml" id="S3.p1.16.m2.1.6b">s</mi><mo xref="S3.p1.16.m2.1.6.cmml" id="S3.p1.16.m2.1.6c">)</mo></mrow><mo xref="S3.p1.16.m2.1.17.1.2.1.cmml" id="S3.p1.16.m2.1.17.1.2.1a">‚Å¢</mo><mi xref="S3.p1.16.m2.1.8.cmml" id="S3.p1.16.m2.1.8">d</mi><mo xref="S3.p1.16.m2.1.17.1.2.1.cmml" id="S3.p1.16.m2.1.17.1.2.1b">‚Å¢</mo><mi xref="S3.p1.16.m2.1.9.cmml" id="S3.p1.16.m2.1.9">s</mi></mrow></mrow><mo xref="S3.p1.16.m2.1.10.cmml" id="S3.p1.16.m2.1.10">=</mo><mrow xref="S3.p1.16.m2.1.17.2.cmml" id="S3.p1.16.m2.1.17.2"><mi xref="S3.p1.16.m2.1.11.cmml" id="S3.p1.16.m2.1.11">f</mi><mo xref="S3.p1.16.m2.1.12.cmml" id="S3.p1.16.m2.1.12">-</mo><mrow xref="S3.p1.16.m2.1.17.2.1.cmml" id="S3.p1.16.m2.1.17.2.1"><mn xref="S3.p1.16.m2.1.13.cmml" id="S3.p1.16.m2.1.13">1</mn><mo xref="S3.p1.16.m2.1.14.cmml" id="S3.p1.16.m2.1.14">/</mo><msup xref="S3.p1.16.m2.1.17.2.1.1.cmml" id="S3.p1.16.m2.1.17.2.1.1"><mi xref="S3.p1.16.m2.1.15.cmml" id="S3.p1.16.m2.1.15">Œª</mi><mrow xref="S3.p1.16.m2.1.16.1.cmml" id="S3.p1.16.m2.1.16.1"><mi xref="S3.p1.16.m2.1.16.1.1.cmml" id="S3.p1.16.m2.1.16.1.1">p</mi><mo xref="S3.p1.16.m2.1.16.1.2.cmml" id="S3.p1.16.m2.1.16.1.2">-</mo><mn xref="S3.p1.16.m2.1.16.1.3.cmml" id="S3.p1.16.m2.1.16.1.3">1</mn></mrow></msup></mrow></mrow></mrow><annotation-xml xref="S3.p1.16.m2.1" id="S3.p1.16.m2.1.cmml" encoding="MathML-Content"><apply xref="S3.p1.16.m2.1.17" id="S3.p1.16.m2.1.17.cmml"><eq xref="S3.p1.16.m2.1.10" id="S3.p1.16.m2.1.10.cmml"></eq><apply xref="S3.p1.16.m2.1.17.1" id="S3.p1.16.m2.1.17.1.cmml"><apply xref="S3.p1.16.m2.1.17.1.1" id="S3.p1.16.m2.1.17.1.1.cmml"><csymbol cd="ambiguous" id="S3.p1.16.m2.1.17.1.1.1.cmml">superscript</csymbol><apply id="S3.p1.16.m2.1.17.1.1.2.cmml"><csymbol cd="ambiguous" id="S3.p1.16.m2.1.17.1.1.2.1.cmml">subscript</csymbol><int xref="S3.p1.16.m2.1.1" id="S3.p1.16.m2.1.1.cmml"></int><cn xref="S3.p1.16.m2.1.2.1" id="S3.p1.16.m2.1.2.1.cmml" type="integer">0</cn></apply><ci xref="S3.p1.16.m2.1.3.1" id="S3.p1.16.m2.1.3.1.cmml">A</ci></apply><apply xref="S3.p1.16.m2.1.17.1.2" id="S3.p1.16.m2.1.17.1.2.cmml"><times xref="S3.p1.16.m2.1.17.1.2.1" id="S3.p1.16.m2.1.17.1.2.1.cmml"></times><ci xref="S3.p1.16.m2.1.4" id="S3.p1.16.m2.1.4.cmml">œï</ci><ci xref="S3.p1.16.m2.1.6" id="S3.p1.16.m2.1.6.cmml">s</ci><ci xref="S3.p1.16.m2.1.8" id="S3.p1.16.m2.1.8.cmml">d</ci><ci xref="S3.p1.16.m2.1.9" id="S3.p1.16.m2.1.9.cmml">s</ci></apply></apply><apply xref="S3.p1.16.m2.1.17.2" id="S3.p1.16.m2.1.17.2.cm