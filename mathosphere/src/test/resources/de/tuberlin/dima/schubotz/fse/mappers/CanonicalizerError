Let us embed <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S4.p9.1.m9.1.cmml" alttext="{\cal C}^{G}" class="ltx_Math" id="S4.p9.1.m9.1" display="inline"><semantics xref="S4.p9.1.m9.1.cmml" id="S4.p9.1.m9.1a"><msup xref="S4.p9.1.m9.1.3.cmml" id="S4.p9.1.m9.1.3"><mi xref="S4.p9.1.m9.1.1.cmml" id="S4.p9.1.m9.1.1">ùíû</mi><mi xref="S4.p9.1.m9.1.2.1.cmml" id="S4.p9.1.m9.1.2.1">G</mi></msup><annotation-xml xref="S4.p9.1.m9.1" id="S4.p9.1.m9.1.cmml" encoding="MathML-Content"><apply xref="S4.p9.1.m9.1.3" id="S4.p9.1.m9.1.3.cmml"><csymbol cd="ambiguous" id="S4.p9.1.m9.1.3.1.cmml">superscript</csymbol><ci xref="S4.p9.1.m9.1.1" id="S4.p9.1.m9.1.1.cmml">ùíû</ci><ci xref="S4.p9.1.m9.1.2.1" id="S4.p9.1.m9.1.2.1.cmml">G</ci></apply></annotation-xml><annotation xref="S4.p9.1.m9.1.cmml" id="S4.p9.1.m9.1b" encoding="application/x-tex">{\cal C}^{G}</annotation></semantics></math> into <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S4.p9.1.m10.1.cmml" alttext="V^{\mathds{C}}_{n_{t},T}" class="ltx_Math" id="S4.p9.1.m10.1" display="inline"><semantics xref="S4.p9.1.m10.1.cmml" id="S4.p9.1.m10.1a"><msubsup xref="S4.p9.1.m10.1.4.cmml" id="S4.p9.1.m10.1.4"><mi xref="S4.p9.1.m10.1.1.cmml" id="S4.p9.1.m10.1.1">V</mi><mrow xref="S4.p9.1.m10.1.3.1.cmml" id="S4.p9.1.m10.1.3.1"><msub xref="S4.p9.1.m10.1.3.1.6.cmml" id="S4.p9.1.m10.1.3.1.6"><mi xref="S4.p9.1.m10.1.3.1.1.cmml" id="S4.p9.1.m10.1.3.1.1">n</mi><mi xref="S4.p9.1.m10.1.3.1.2.1.cmml" id="S4.p9.1.m10.1.3.1.2.1">t</mi></msub><mo xref="S4.p9.1.m10.1.3.1.cmml" id="S4.p9.1.m10.1.3.1a">,</mo><mi xref="S4.p9.1.m10.1.3.1.4.cmml" id="S4.p9.1.m10.1.3.1.4">T</mi></mrow><mi xref="S4.p9.1.m10.1.2.1.cmml" id="S4.p9.1.m10.1.2.1">‚ÑÇ</mi></msubsup><annotation-xml xref="S4.p9.1.m10.1" id="S4.p9.1.m10.1.cmml" encoding="MathML-Content"><apply xref="S4.p9.1.m10.1.4" id="S4.p9.1.m10.1.4.cmml"><csymbol cd="ambiguous" id="S4.p9.1.m10.1.4.1.cmml">subscript</csymbol><apply id="S4.p9.1.m10.1.4.2.cmml"><csymbol cd="ambiguous" id="S4.p9.1.m10.1.4.