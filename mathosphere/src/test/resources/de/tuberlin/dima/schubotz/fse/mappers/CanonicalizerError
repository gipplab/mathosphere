<math xmlns="http://www.w3.org/1998/Math/MathML" xref="S4.p9.3.m4.1.cmml" alttext="F(\vartheta)=2^{k}\prod_{i}(1-\cos\vartheta_{i})-\lambda(\sum_{i}\sin^{2}%&#10;\vartheta_{i}-\delta)" class="ltx_Math" id="S4.p9.3.m4.1" display="inline"><semantics xref="S4.p9.3.m4.1.cmml" id="S4.p9.3.m4.1a"><mrow xref="S4.p9.3.m4.1.29.cmml" id="S4.p9.3.m4.1.29"><mrow xref="S4.p9.3.m4.1.29.1.cmml" id="S4.p9.3.m4.1.29.1"><mi xref="S4.p9.3.m4.1.1.cmml" id="S4.p9.3.m4.1.1">F</mi><mo xref="S4.p9.3.m4.1.29.1.1.cmml" id="S4.p9.3.m4.1.29.1.1">‚Å¢</mo><mrow xref="S4.p9.3.m4.1.3.cmml" id="S4.p9.3.m4.1.3"><mo xref="S4.p9.3.m4.1.3.cmml" id="S4.p9.3.m4.1.3a">(</mo><mi xref="S4.p9.3.m4.1.3.cmml" id="S4.p9.3.m4.1.3b">œë</mi><mo xref="S4.p9.3.m4.1.3.cmml" id="S4.p9.3.m4.1.3c">)</mo></mrow></mrow><mo xref="S4.p9.3.m4.1.5.cmml" id="S4.p9.3.m4.1.5">=</mo><mrow xref="S4.p9.3.m4.1.29.2.cmml" id="S4.p9.3.m4.1.29.2"><mrow xref="S4.p9.3.m4.1.29.2.1.cmml" id="S4.p9.3.m4.1.29.2.1"><msup xref="S4.p9.3.m4.1.29.2.1.2.cmml" id="S4.p9.3.m4.1.29.2.1.2"><mn xref="S4.p9.3.m4.1.6.cmml" id="S4.p9.3.m4.1.6">2</mn><mi xref="S4.p9.3.m4.1.7.1.cmml" id="S4.p9.3.m4.1.7.1">k</mi></msup><mo xref="S4.p9.3.m4.1.29.2.1.1.cmml" id="S4.p9.3.m4.1.29.2.1.1">‚Å¢</mo><mrow xref="S4.p9.3.m4.1.29.2.1.3.cmml" id="S4.p9.3.m4.1.29.2.1.3"><msub xref="S4.p9.3.m4.1.29.2.1.3.1.cmml" id="S4.p9.3.m4.1.29.2.1.3.1"><mo xref="S4.p9.3.m4.1.8.cmml" id="S4.p9.3.m4.1.8">‚àè</mo><mi xref="S4.p9.3.m4.1.9.1.cmml" id="S4.p9.3.m4.1.9.1">i</mi></msub><mrow xref="S4.p9.3.m4.1.29.2.1.3.2.cmml" id="S4.p9.3.m4.1.29.2.1.3.2"><mo xref="S4.p9.3.m4.1.29.2.1.3.2.cmml" id="S4.p9.3.m4.1.29.2.1.3.2a">(</mo><mrow xref="S4.p9.3.m4.1.29.2.1.3.2.cmml" id="S4.p9.3.m4.1.29.2.1.3.2b"><mn xref="S4.p9.3.m4.1.11.cmml" id="S4.p9.3.m4.1.11">1</mn><mo xref="S4.p9.3.m4.1.12.cmml" id="S4.p9.3.m4.1.12">-</mo><mrow xref="S4.p9.3.m4.1.29.2.1.3.2.1.cmml" id="S4.p9.3.m4.1.29.2.1.3.2.1"><mi xref="S4.p9.3.m4.1.13.cmml" id="S4.p9.3.m4.1.13">cos</mi><mo xref="S4.p9.3.m4.1.29.2.1.3.2.1.cmml" id