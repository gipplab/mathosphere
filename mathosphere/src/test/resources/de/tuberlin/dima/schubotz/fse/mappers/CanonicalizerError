while it is 0 if <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S2.p22.2.m8.1.cmml" alttext="p&lt;1/\liminf_{n\rightarrow\infty}(\theta_{n}{\mathbb{E}}[T_{n}])^{1/n}." class="ltx_Math" id="S2.p22.2.m8.1" display="inline"><semantics xref="S2.p22.2.m8.1.cmml" id="S2.p22.2.m8.1a"><mrow xref="S2.p22.2.m8.1.18.cmml" id="S2.p22.2.m8.1.18"><mrow xref="S2.p22.2.m8.1.18.cmml" id="S2.p22.2.m8.1.18a"><mi xref="S2.p22.2.m8.1.1.cmml" id="S2.p22.2.m8.1.1">p</mi><mo xref="S2.p22.2.m8.1.2.cmml" id="S2.p22.2.m8.1.2">&lt;</mo><mrow xref="S2.p22.2.m8.1.18.1.cmml" id="S2.p22.2.m8.1.18.1"><mn xref="S2.p22.2.m8.1.3.cmml" id="S2.p22.2.m8.1.3">1</mn><mo xref="S2.p22.2.m8.1.4.cmml" id="S2.p22.2.m8.1.4">/</mo><mrow xref="S2.p22.2.m8.1.18.1.1.cmml" id="S2.p22.2.m8.1.18.1.1"><msub xref="S2.p22.2.m8.1.18.1.1.1.cmml" id="S2.p22.2.m8.1.18.1.1.1"><mo xref="S2.p22.2.m8.1.5.cmml" id="S2.p22.2.m8.1.5">lim inf</mo><mrow xref="S2.p22.2.m8.1.6.1.cmml" id="S2.p22.2.m8.1.6.1"><mi xref="S2.p22.2.m8.1.6.1.1.cmml" id="S2.p22.2.m8.1.6.1.1">n</mi><mo xref="S2.p22.2.m8.1.6.1.2.cmml" id="S2.p22.2.m8.1.6.1.2">‚Üí</mo><mi xref="S2.p22.2.m8.1.6.1.3.cmml" mathvariant="normal" id="S2.p22.2.m8.1.6.1.3">‚àû</mi></mrow></msub><mo xref="S2.p22.2.m8.1.18.1.1.cmml" id="S2.p22.2.m8.1.18.1.1a">‚Å°</mo><msup xref="S2.p22.2.m8.1.18.1.1.2.cmml" id="S2.p22.2.m8.1.18.1.1.2"><mrow xref="S2.p22.2.m8.1.18.1.1.2.2.cmml" id="S2.p22.2.m8.1.18.1.1.2.2"><mo xref="S2.p22.2.m8.1.18.1.1.2.2.cmml" id="S2.p22.2.m8.1.18.1.1.2.2a">(</mo><mrow xref="S2.p22.2.m8.1.18.1.1.2.2.cmml" id="S2.p22.2.m8.1.18.1.1.2.2b"><msub xref="S2.p22.2.m8.1.18.1.1.2.2.2.cmml" id="S2.p22.2.m8.1.18.1.1.2.2.2"><mi xref="S2.p22.2.m8.1.8.cmml" id="S2.p22.2.m8.1.8">Œ∏</mi><mi xref="S2.p22.2.m8.1.9.1.cmml" id="S2.p22.2.m8.1.9.1">n</mi></msub><mo xref="S2.p22.2.m8.1.18.1.1.2.2.1.cmml" id="S2.p22.2.m8.1.18.1.1.2.2.1">‚Å¢</mo><mi xref="S2.p22.2.m8.1.10.cmml" id="S2.p22.2.m8.1.10">ùîº</mi><mo xref="S2.p22.2.m8.1.18.1.1.2.2.1.cmml" id="S2.p22.2.m8.1.18.1.1.2.2.1a">‚Å¢</