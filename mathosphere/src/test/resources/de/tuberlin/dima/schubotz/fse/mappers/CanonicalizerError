<br class="ltx_break"></br><span class="ltx_text ltx_font_bold" id="p1.11.1.1">TH√âOR√àME 3</span>. Pour <math xmlns="http://www.w3.org/1998/Math/MathML" xref="p1.11.1.m4.1.cmml" alttext="\nu&gt;-\frac{1}{2}" class="ltx_Math" id="p1.11.1.m4.1" display="inline"><semantics xref="p1.11.1.m4.1.cmml" id="p1.11.1.m4.1a"><mrow xref="p1.11.1.m4.1.5.cmml" id="p1.11.1.m4.1.5"><mi xref="p1.11.1.m4.1.1.cmml" mathsize="normal" id="p1.11.1.m4.1.1">ŒΩ</mi><mo xref="p1.11.1.m4.1.2.cmml" mathsize="normal" id="p1.11.1.m4.1.2">&gt;</mo><mrow xref="p1.11.1.m4.1.5.1.cmml" id="p1.11.1.m4.1.5.1"><mo xref="p1.11.1.m4.1.3.cmml" mathsize="normal" id="p1.11.1.m4.1.3">-</mo><mfrac xref="p1.11.1.m4.1.4.cmml" id="p1.11.1.m4.1.4"><mn xref="p1.11.1.m4.1.4.2.cmml" mathsize="normal" id="p1.11.1.m4.1.4.2">1</mn><mn xref="p1.11.1.m4.1.4.3.cmml" mathsize="normal" id="p1.11.1.m4.1.4.3">2</mn></mfrac></mrow></mrow><annotation-xml xref="p1.11.1.m4.1" id="p1.11.1.m4.1.cmml" encoding="MathML-Content"><apply xref="p1.11.1.m4.1.5" id="p1.11.1.m4.1.5.cmml"><gt xref="p1.11.1.m4.1.2" id="p1.11.1.m4.1.2.cmml"></gt><ci xref="p1.11.1.m4.1.1" id="p1.11.1.m4.1.1.cmml">ŒΩ</ci><apply xref="p1.11.1.m4.1.5.1" id="p1.11.1.m4.1.5.1.cmml"><minus xref="p1.11.1.m4.1.3" id="p1.11.1.m4.1.3.cmml"></minus><apply xref="p1.11.1.m4.1.4" id="p1.11.1.m4.1.4.cmml"><divide id="p1.11.1.m4.1.4.1.cmml"></divide><cn xref="p1.11.1.m4.1.4.2" id="p1.11.1.m4.1.4.2.cmml" type="integer">1</cn><cn xref="p1.11.1.m4.1.4.3" id="p1.11.1.m4.1.4.3.cmml" type="integer">2</cn></apply></apply></apply></annotation-xml><annotation xref="p1.11.1.m4.1.cmml" id="p1.11.1.m4.1b" encoding="application/x-tex">\nu&gt;-\frac{1}{2}</annotation></semantics></math> et <math xmlns="http://www.w3.org/1998/Math/MathML" xref="p1.11.1.m5.1.cmml" alttext="0&lt;\mu&lt;\frac{1}{2}" class="ltx_Math" id="p1.11.1.m5.1" display="inline"><semantics xref="p1.11.1.m5.1.cmml" id="p1.11.1.m5.1a"><mrow xref="p1.11.1.m5.1.6.cmml" id="p1.11.1.m5.1.6"><mn xref="p1.11.1.m5.1.1.cmml" mathsize