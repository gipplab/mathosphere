function defined by the following way: <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.15.m13.1.cmml" alttext="\phi:[0,1-1/\lambda^{p}]\rightarrow\mathbb{R}^{+}" class="ltx_Math" id="S3.p1.15.m13.1" display="inline"><semantics xref="S3.p1.15.m13.1.cmml" id="S3.p1.15.m13.1a"><mrow xref="S3.p1.15.m13.1.16.cmml" id="S3.p1.15.m13.1.16"><mi xref="S3.p1.15.m13.1.1.cmml" id="S3.p1.15.m13.1.1">œï</mi><mo xref="S3.p1.15.m13.1.2.cmml" id="S3.p1.15.m13.1.2">:</mo><mrow xref="S3.p1.15.m13.1.16.1.cmml" id="S3.p1.15.m13.1.16.1"><mrow xref="S3.p1.15.m13.1.16.1.1.cmml" id="S3.p1.15.m13.1.16.1.1"><mo xref="S3.p1.15.m13.1.16.1.1.cmml" id="S3.p1.15.m13.1.16.1.1a">[</mo><mrow xref="S3.p1.15.m13.1.16.1.1.cmml" id="S3.p1.15.m13.1.16.1.1b"><mn xref="S3.p1.15.m13.1.4.cmml" id="S3.p1.15.m13.1.4">0</mn><mo xref="S3.p1.15.m13.1.16.1.1.cmml" id="S3.p1.15.m13.1.16.1.1c">,</mo><mrow xref="S3.p1.15.m13.1.16.1.1.2.cmml" id="S3.p1.15.m13.1.16.1.1.2"><mn xref="S3.p1.15.m13.1.6.cmml" id="S3.p1.15.m13.1.6">1</mn><mo xref="S3.p1.15.m13.1.7.cmml" id="S3.p1.15.m13.1.7">-</mo><mrow xref="S3.p1.15.m13.1.16.1.1.2.1.cmml" id="S3.p1.15.m13.1.16.1.1.2.1"><mn xref="S3.p1.15.m13.1.8.cmml" id="S3.p1.15.m13.1.8">1</mn><mo xref="S3.p1.15.m13.1.9.cmml" id="S3.p1.15.m13.1.9">/</mo><msup xref="S3.p1.15.m13.1.16.1.1.2.1.1.cmml" id="S3.p1.15.m13.1.16.1.1.2.1.1"><mi xref="S3.p1.15.m13.1.10.cmml" id="S3.p1.15.m13.1.10">Œª</mi><mi xref="S3.p1.15.m13.1.11.1.cmml" id="S3.p1.15.m13.1.11.1">p</mi></msup></mrow></mrow></mrow><mo xref="S3.p1.15.m13.1.16.1.1.cmml" id="S3.p1.15.m13.1.16.1.1d">]</mo></mrow><mo xref="S3.p1.15.m13.1.13.cmml" id="S3.p1.15.m13.1.13">‚Üí</mo><msup xref="S3.p1.15.m13.1.16.1.2.cmml" id="S3.p1.15.m13.1.16.1.2"><mi xref="S3.p1.15.m13.1.14.cmml" id="S3.p1.15.m13.1.14">‚Ñù</mi><mo xref="S3.p1.15.m13.1.15.1.cmml" id="S3.p1.15.m13.1.15.1">+</mo></msup></mrow></mrow><annotation-xml xref="S3.p1.15.m13.1" id="S3.p1.15.m13.1.cmml" encoding="MathML-Content"><apply xref="S3.p1.15.m13.1.16" id="S3.p1.15.m