to <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.5.m2.1.cmml" alttext="\mu(\left\{\mathcal{M}\phi\geq\lambda\right\})=\mu(K)\leq 1/\lambda^{p}" class="ltx_Math" id="S3.p1.5.m2.1" display="inline"><semantics xref="S3.p1.5.m2.1.cmml" id="S3.p1.5.m2.1a"><mrow xref="S3.p1.5.m2.1.cmml" id="S3.p1.5.m2.1b"><mi id="S3.p1.5.m2.1.1">Œº</mi><mrow xref="S3.p1.5.m2.1.cmml" id="S3.p1.5.m2.1c"><mo xref="S3.p1.5.m2.1.2.cmml" id="S3.p1.5.m2.1.2">(</mo><mrow xref="S3.p1.5.m2.1.cmml" id="S3.p1.5.m2.1d"><mo xref="S3.p1.5.m2.1.3.cmml" id="S3.p1.5.m2.1.3">{</mo><mi id="S3.p1.5.m2.1.4">‚Ñ≥</mi><mi id="S3.p1.5.m2.1.5">œï</mi><mo xref="S3.p1.5.m2.1.6.cmml" id="S3.p1.5.m2.1.6">‚â•</mo><mi id="S3.p1.5.m2.1.7">Œª</mi><mo xref="S3.p1.5.m2.1.8.cmml" id="S3.p1.5.m2.1.8">}</mo></mrow><mo xref="S3.p1.5.m2.1.9.cmml" id="S3.p1.5.m2.1.9">)</mo></mrow><mo xref="S3.p1.5.m2.1.10.cmml" id="S3.p1.5.m2.1.10">=</mo><mi id="S3.p1.5.m2.1.11">Œº</mi><mrow xref="S3.p1.5.m2.1.cmml" id="S3.p1.5.m2.1e"><mo xref="S3.p1.5.m2.1.12.cmml" id="S3.p1.5.m2.1.12">(</mo><mi id="S3.p1.5.m2.1.13">K</mi><mo xref="S3.p1.5.m2.1.14.cmml" id="S3.p1.5.m2.1.14">)</mo></mrow><mo xref="S3.p1.5.m2.1.15.cmml" id="S3.p1.5.m2.1.15">‚â§</mo><mn xref="S3.p1.5.m2.1.16.cmml" id="S3.p1.5.m2.1.16">1</mn><mo xref="S3.p1.5.m2.1.17.cmml" id="S3.p1.5.m2.1.17">/</mo><msup xref="S3.p1.5.m2.1.20.cmml" id="S3.p1.5.m2.1.20"><mi xref="S3.p1.5.m2.1.18.cmml" id="S3.p1.5.m2.1.18">Œª</mi><mi xref="S3.p1.5.m2.1.19.1.cmml" id="S3.p1.5.m2.1.19.1">p</mi></msup></mrow><annotation-xml xref="S3.p1.5.m2.1" id="S3.p1.5.m2.1.cmml" encoding="MathML-Content"><cerror xref="S3.p1.5.m2.1" id="S3.p1.5.m2.1a.cmml"><csymbol xref="S3.p1.5.m2.1" cd="ambiguous" id="S3.p1.5.m2.1b.cmml">fragments</csymbol><csymbol xref="S3.p1.5.m2.1" cd="unknown" id="S3.p1.5.m2.1c.cmml">Œº</csymbol><cerror xref="S3.p1.5.m2.1" id="S3.p1.5.m2.1d.cmml"><csymbol xref="S3.p1.5.m2.1" cd="ambiguous" id="S3.p1.5.m2.1e.cmml">fragments</csymbol><ci xref="S3.p1.5.m2.1.2" id="S3.p1.5.m2.1.2.cmml