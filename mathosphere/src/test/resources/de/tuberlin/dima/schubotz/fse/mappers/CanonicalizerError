with <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.4.m7.1.cmml" alttext="|||\phi|||_{p,\infty}\leq 1" class="ltx_Math" id="S3.p1.4.m7.1" display="inline"><semantics xref="S3.p1.4.m7.1.cmml" id="S3.p1.4.m7.1a"><mrow xref="S3.p1.4.m7.1.11.cmml" id="S3.p1.4.m7.1.11"><msub xref="S3.p1.4.m7.1.11.1.cmml" id="S3.p1.4.m7.1.11.1"><mrow xref="S3.p1.4.m7.1.11.1.2.cmml" id="S3.p1.4.m7.1.11.1.2"><mo xref="S3.p1.4.m7.1.11.1.2.cmml" id="S3.p1.4.m7.1.11.1.2a" fence="true">||</mo><mrow xref="S3.p1.4.m7.1.11.1.2.2.cmml" id="S3.p1.4.m7.1.11.1.2.2"><mo xref="S3.p1.4.m7.1.11.1.2.2.cmml" id="S3.p1.4.m7.1.11.1.2.2a" fence="true">|</mo><mi xref="S3.p1.4.m7.1.4.cmml" id="S3.p1.4.m7.1.4">œï</mi><mo xref="S3.p1.4.m7.1.11.1.2.2.cmml" id="S3.p1.4.m7.1.11.1.2.2b" fence="true">|</mo></mrow><mo xref="S3.p1.4.m7.1.11.1.2.cmml" id="S3.p1.4.m7.1.11.1.2b" fence="true">||</mo></mrow><mrow xref="S3.p1.4.m7.1.8.1.cmml" id="S3.p1.4.m7.1.8.1"><mi xref="S3.p1.4.m7.1.8.1.1.cmml" id="S3.p1.4.m7.1.8.1.1">p</mi><mo xref="S3.p1.4.m7.1.8.1.cmml" id="S3.p1.4.m7.1.8.1a">,</mo><mi xref="S3.p1.4.m7.1.8.1.3.cmml" mathvariant="normal" id="S3.p1.4.m7.1.8.1.3">‚àû</mi></mrow></msub><mo xref="S3.p1.4.m7.1.9.cmml" id="S3.p1.4.m7.1.9">‚â§</mo><mn xref="S3.p1.4.m7.1.10.cmml" id="S3.p1.4.m7.1.10">1</mn></mrow><annotation-xml xref="S3.p1.4.m7.1" id="S3.p1.4.m7.1.cmml" encoding="MathML-Content"><apply xref="S3.p1.4.m7.1.11" id="S3.p1.4.m7.1.11.cmml"><leq xref="S3.p1.4.m7.1.9" id="S3.p1.4.m7.1.9.cmml"></leq><apply xref="S3.p1.4.m7.1.11.1" id="S3.p1.4.m7.1.11.1.cmml"><csymbol cd="ambiguous" id="S3.p1.4.m7.1.11.1.1.cmml">subscript</csymbol><apply xref="S3.p1.4.m7.1.11.1.2" id="S3.p1.4.m7.1.11.1.2.cmml"><csymbol cd="latexml" id="S3.p1.4.m7.1.11.1.2.1.cmml">norm</csymbol><apply xref="S3.p1.4.m7.1.11.1.2.2" id="S3.p1.4.m7.1.11.1.2.2.cmml"><abs id="S3.p1.4.m7.1.11.1.2.2.1.cmml"></abs><ci xref="S3.p1.4.m7.1.4" id="S3.p1.4.m7.1.4.cmml">œï</ci></apply></apply><apply xref="S3.p1.4.m7.1.8.1" id="S3.p1.4.m7.1.8.1.cmml"><list 