\\
2b(N)(\frac{1}{t}\frac{\partial}{\partial t})^{N-1}[t^{2N-1}\int_{\left\{\left%
|y\right|&lt;1\right\}}\frac{\Psi(x-ty)}{\sqrt{1-\left|y\right|^{2}}}dy]</annotation></semantics></math> si <math xmlns="http://www.w3.org/1998/Math/MathML" xref="p1.33.1.m25.1.cmml" alttext="n" class="ltx_Math" id="p1.33.1.m25.1" display="inline"><semantics xref="p1.33.1.m25.1.cmml" id="p1.33.1.m25.1a"><mi xref="p1.33.1.m25.1.1.cmml" mathsize="normal" id="p1.33.1.m25.1.1">n</mi><annotation-xml xref="p1.33.1.m25.1" id="p1.33.1.m25.1.cmml" encoding="MathML-Content"><ci xref="p1.33.1.m25.1.1" id="p1.33.1.m25.1.1.cmml">n</ci></annotation-xml><annotation xref="p1.33.1.m25.1.cmml" id="p1.33.1.m25.1b" encoding="application/x-tex">n</annotation></semantics></math> est pair <math xmlns="http://www.w3.org/1998/Math/MathML" xref="p1.33.1.m26.1.cmml" alttext="(n=2N)" class="ltx_Math" id="p1.33.1.m26.1" display="inline"><semantics xref="p1.33.1.m26.1.cmml" id="p1.33.1.m26.1a"><mrow xref="p1.33.1.m26.1.7.cmml" id="p1.33.1.m26.1.7"><mo xref="p1.33.1.m26.1.7.cmml" mathsize="small" id="p1.33.1.m26.1.7a">(</mo><mrow xref="p1.33.1.m26.1.7.cmml" id="p1.33.1.m26.1.7b"><mi xref="p1.33.1.m26.1.2.cmml" mathsize="normal" id="p1.33.1.m26.1.2">n</mi><mo xref="p1.33.1.m26.1.3.cmml" mathsize="normal" id="p1.33.1.m26.1.3">=</mo><mrow xref="p1.33.1.m26.1.7.1.cmml" id="p1.33.1.m26.1.7.1"><mn xref="p1.33.1.m26.1.4.cmml" mathsize="normal" id="p1.33.1.m26.1.4">2</mn><mo xref="p1.33.1.m26.1.7.1.1.cmml" mathsize="small" id="p1.33.1.m26.1.7.1.1">‚Å¢</mo><mi xref="p1.33.1.m26.1.5.cmml" mathsize="normal" id="p1.33.1.m26.1.5">N</mi></mrow></mrow><mo xref="p1.33.1.m26.1.7.cmml" mathsize="small" id="p1.33.1.m26.1.7c">)</mo></mrow><annotation-xml xref="p1.33.1.m26.1" id="p1.33.1.m26.1.cmml" encoding="MathML-Content"><apply xref="p1.33.1.m26.1.7" id="p1.33.1.m26.1.7.cmml"><eq xref="p1.33.1.m26.1.3" id="p1.33.1.m26.1.3.cmml"></eq><ci xref="p1.33.1.m26.1.2" id="p1.33.1.m26.1.2.cmml">n</ci><apply xref="p1.33.1.m26.1.7.1" id="p1.33.1.m26.1.7.1.cmml"><times xref="p1.33.1.m26.1.7.1.1" id="p1.33.1.m26.1.7.1.1.cmml"></times><cn xref="p1.33.1.m26.1.4" id="p1.33.1.m26.1.4.cmml" type="integer">2</cn><ci xref="p1.33.1.m26.1.5" id="p1.33.1.m26.1.5.cmml">N</ci></apply></apply></annotation-xml><annotation xref="p1.33.1.m26.1.cmml" id="p1.33.1.m26.1b" encoding="application/x-tex">(n=2N)</annotation></semantics></math>.
<br class="ltx_break"></br><span class="ltx_text ltx_font_bold" id="p1.33.1.5">Preuve</span>. On distingue deux cas:
<br class="ltx_break"></br><span class="ltx_text ltx_font_bold" id="p1.33.1.6">- Cas <math xmlns="http://www.w3.org/1998/Math/MathML" xref="p1.33.1.6.m1.1.cmml" alttext="n" class="ltx_Math" id="p1.33.1.6.m1.1" display="inline"><semantics xref="p1.33.1.6.m1.1.cmml" id="p1.33.1.6.m1.1a"><mi xref="p1.33.1.6.m1.1.1.cmml" mathsize="normal" id="p1.33.1.6.m1.1.1">n</mi><annotation-xml xref="p1.33.1.6.m1.1" id="p1.33.1.6.m1.1.cmml" encoding="MathML-Content"><ci xref="p1.33.1.6.m1.1.1" id="p1.33.1.6.m1.1.1.cmml">n</ci></annotation-xml><annotation xref="p1.33.1.6.m1.1.cmml" id="p1.33.1.6.m1.1b" encoding="application/x-tex">n</annotation></semantics></math> impair <math xmlns="http://www.w3.org/1998/Math/MathML" xref="p1.33.1.6.m2.1.cmml" alttext="(n=2N+1)" class="ltx_Math" id="p1.33.1.6.m2.1" display="inline"><semantics xref="p1.33.1.6.m2.1.cmml" id="p1.33.1.6.m2.1a"><mrow xref="p1.33.1.6.m2.1.9.cmml" id="p1.33.1.6.m2.1.9"><mo xref="p1.33.1.6.m2.1.9.cmml" mathsize="small" mathvariant="bold" id="p1.33.1.6.m2.1.9a">(</mo><mrow xref="p1.33.1.6.m2.1.9.cmml" id="p1.33.1.6.m2.1.9b"><mi xref="p1.33.1.6.m2.1.2.cmml" mathsize="normal" id="p1.33.1.6.m2.1.2">n</mi><mo xref="p1.33.1.6.m2.1.3.cmml" mathsize="normal" mathvariant="normal" id="p1.33.1.6.m2.1.3">=</mo><mrow xref="p1.33.1.6.m2.1.9.1.cmml" id="p1.33.1.6.m2.1.9.1"><mrow xref="p1.33.1.6.m2.1.9.1.1.cmml" id="p1.33.1.6.m2.1.9.1.1"><mn xref="p1.33.1.6.m2.1.4.cmml" mathsize="normal" mathvariant="normal" id="p1.33.1.6.m2.1.4">2</mn><mo xref="p1.33.1.6.m2.1.9.1.1.1.cmml" mathsize="small" mathvariant="bold" id="p1.33.1.6.m2.1.9.1.1.1">‚Å¢</mo><mi xref="p1.33.1.6.m2.1.5.cmml" mathsize="normal" id="p1.33.1.6.m2.1.5">N</mi></mrow><mo xref="p1.33.1.6.m2.1.6.cmml" mathsize="normal" mathvariant="normal" id="p1.33.1.6.m2.1.6">+</mo><mn xref="p1.33.1.6.m2.1.7.cmml" mathsize="normal" mathvariant="normal" id="p1.33.1.6.m2.1.7">1</mn></mrow></mrow><mo xref="p1.33.1.6.m2.1.9.cmml" mathsize="small" mathvariant="bold" id="p1.33.1.6.m2.1.9c">)</mo></m