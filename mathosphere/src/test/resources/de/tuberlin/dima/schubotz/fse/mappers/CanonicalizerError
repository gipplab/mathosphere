p-1}}</annotation></semantics></math> thus <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.7.m2.1.cmml" alttext="\int_{0}^{1}\phi(s)ds=f" class="ltx_Math" id="S3.p1.7.m2.1" display="inline"><semantics xref="S3.p1.7.m2.1.cmml" id="S3.p1.7.m2.1a"><mrow xref="S3.p1.7.m2.1.12.cmml" id="S3.p1.7.m2.1.12"><mrow xref="S3.p1.7.m2.1.12.1.cmml" id="S3.p1.7.m2.1.12.1"><msubsup xref="S3.p1.7.m2.1.12.1.1.cmml" id="S3.p1.7.m2.1.12.1.1"><mo xref="S3.p1.7.m2.1.1.cmml" id="S3.p1.7.m2.1.1">‚à´</mo><mn xref="S3.p1.7.m2.1.2.1.cmml" id="S3.p1.7.m2.1.2.1">0</mn><mn xref="S3.p1.7.m2.1.3.1.cmml" id="S3.p1.7.m2.1.3.1">1</mn></msubsup><mrow xref="S3.p1.7.m2.1.12.1.2.cmml" id="S3.p1.7.m2.1.12.1.2"><mi xref="S3.p1.7.m2.1.4.cmml" id="S3.p1.7.m2.1.4">œï</mi><mo xref="S3.p1.7.m2.1.12.1.2.1.cmml" id="S3.p1.7.m2.1.12.1.2.1">‚Å¢</mo><mrow xref="S3.p1.7.m2.1.6.cmml" id="S3.p1.7.m2.1.6"><mo xref="S3.p1.7.m2.1.6.cmml" id="S3.p1.7.m2.1.6a">(</mo><mi xref="S3.p1.7.m2.1.6.cmml" id="S3.p1.7.m2.1.6b">s</mi><mo xref="S3.p1.7.m2.1.6.cmml" id="S3.p1.7.m2.1.6c">)</mo></mrow><mo xref="S3.p1.7.m2.1.12.1.2.1.cmml" id="S3.p1.7.m2.1.12.1.2.1a">‚Å¢</mo><mi xref="S3.p1.7.m2.1.8.cmml" id="S3.p1.7.m2.1.8">d</mi><mo xref="S3.p1.7.m2.1.12.1.2.1.cmml" id="S3.p1.7.m2.1.12.1.2.1b">‚Å¢</mo><mi xref="S3.p1.7.m2.1.9.cmml" id="S3.p1.7.m2.1.9">s</mi></mrow></mrow><mo xref="S3.p1.7.m2.1.10.cmml" id="S3.p1.7.m2.1.10">=</mo><mi xref="S3.p1.7.m2.1.11.cmml" id="S3.p1.7.m2.1.11">f</mi></mrow><annotation-xml xref="S3.p1.7.m2.1" id="S3.p1.7.m2.1.cmml" encoding="MathML-Content"><apply xref="S3.p1.7.m2.1.12" id="S3.p1.7.m2.1.12.cmml"><eq xref="S3.p1.7.m2.1.10" id="S3.p1.7.m2.1.10.cmml"></eq><apply xref="S3.p1.7.m2.1.12.1" id="S3.p1.7.m2.1.12.1.cmml"><apply xref="S3.p1.7.m2.1.12.1.1" id="S3.p1.7.m2.1.12.1.1.cmml"><csymbol cd="ambiguous" id="S3.p1.7.m2.1.12.1.1.1.cmml">superscript</csymbol><apply id="S3.p1.7.m2.1.12.1.1.2.cmml"><csymbol cd="ambiguous" id="S3.p1.7.m2.1.12.1.1.2.1.cmml">subscript</csymbol><int xref="S3.p1.7.m2.1.1"