3" id="I1.i1.p1.1.m1.1.33.cmml">a</ci><apply xref="I1.i1.p1.1.m1.1.34.1" id="I1.i1.p1.1.m1.1.34.1.cmml"><minus xref="I1.i1.p1.1.m1.1.34.1.2" id="I1.i1.p1.1.m1.1.34.1.2.cmml"></minus><ci xref="I1.i1.p1.1.m1.1.34.1.1" id="I1.i1.p1.1.m1.1.34.1.1.cmml">j</ci><cn xref="I1.i1.p1.1.m1.1.34.1.3" id="I1.i1.p1.1.m1.1.34.1.3.cmml" type="integer">1</cn></apply></apply><cn xref="I1.i1.p1.1.m1.1.35.1" id="I1.i1.p1.1.m1.1.35.1.cmml" type="integer">2</cn></apply><apply xref="I1.i1.p1.1.m1.1.39.4.2.2.2.3" id="I1.i1.p1.1.m1.1.39.4.2.2.2.3.cmml"><csymbol cd="ambiguous" id="I1.i1.p1.1.m1.1.39.4.2.2.2.3.1.cmml">superscript</csymbol><ci xref="I1.i1.p1.1.m1.1.36" id="I1.i1.p1.1.m1.1.36.cmml">x</ci><ci xref="I1.i1.p1.1.m1.1.37.1" id="I1.i1.p1.1.m1.1.37.1.cmml">j</ci></apply></apply></apply></apply></apply></apply></apply></annotation-xml><annotation xref="I1.i1.p1.1.m1.1.cmml" id="I1.i1.p1.1.m1.1b" encoding="application/x-tex">\sum\limits_{n\geq\ell}a_{n}^{2}x^{n}=\frac{1}{x}\sum\limits_{n\geq\ell}f_{0}(%
n+1)x^{n+1}=\frac{1}{x}\left(F_{0}(x)-\sum\limits_{j=1}^{\ell}a_{j-1}^{2}x^{j}\right)</annotation></semantics></math>,</p>
</div>
</li>
<li style="list-style-type:none;" class="ltx_item" id="I1.i2">
<span class="ltx_tag ltx_tag_enumerate">2.</span> 
<div class="ltx_para" id="I1.i2.p1">
