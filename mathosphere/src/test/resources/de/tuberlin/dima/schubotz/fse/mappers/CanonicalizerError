have <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.15.m3.1.cmml" alttext="B(f,F,\lambda)\geq 1/\lambda^{p}" class="ltx_Math" id="S3.p1.15.m3.1" display="inline"><semantics xref="S3.p1.15.m3.1.cmml" id="S3.p1.15.m3.1a"><mrow xref="S3.p1.15.m3.1.14.cmml" id="S3.p1.15.m3.1.14"><mrow xref="S3.p1.15.m3.1.14.1.cmml" id="S3.p1.15.m3.1.14.1"><mi xref="S3.p1.15.m3.1.1.cmml" id="S3.p1.15.m3.1.1">B</mi><mo xref="S3.p1.15.m3.1.14.1.1.cmml" id="S3.p1.15.m3.1.14.1.1">‚Å¢</mo><mrow xref="S3.p1.15.m3.1.14.1.2.cmml" id="S3.p1.15.m3.1.14.1.2"><mo xref="S3.p1.15.m3.1.14.1.2.cmml" id="S3.p1.15.m3.1.14.1.2a">(</mo><mrow xref="S3.p1.15.m3.1.14.1.2.cmml" id="S3.p1.15.m3.1.14.1.2b"><mi xref="S3.p1.15.m3.1.3.cmml" id="S3.p1.15.m3.1.3">f</mi><mo xref="S3.p1.15.m3.1.14.1.2.cmml" id="S3.p1.15.m3.1.14.1.2c">,</mo><mi xref="S3.p1.15.m3.1.5.cmml" id="S3.p1.15.m3.1.5">F</mi><mo xref="S3.p1.15.m3.1.14.1.2.cmml" id="S3.p1.15.m3.1.14.1.2d">,</mo><mi xref="S3.p1.15.m3.1.7.cmml" id="S3.p1.15.m3.1.7">Œª</mi></mrow><mo xref="S3.p1.15.m3.1.14.1.2.cmml" id="S3.p1.15.m3.1.14.1.2e">)</mo></mrow></mrow><mo xref="S3.p1.15.m3.1.9.cmml" id="S3.p1.15.m3.1.9">‚â•</mo><mrow xref="S3.p1.15.m3.1.14.2.cmml" id="S3.p1.15.m3.1.14.2"><mn xref="S3.p1.15.m3.1.10.cmml" id="S3.p1.15.m3.1.10">1</mn><mo xref="S3.p1.15.m3.1.11.cmml" id="S3.p1.15.m3.1.11">/</mo><msup xref="S3.p1.15.m3.1.14.2.1.cmml" id="S3.p1.15.m3.1.14.2.1"><mi xref="S3.p1.15.m3.1.12.cmml" id="S3.p1.15.m3.1.12">Œª</mi><mi xref="S3.p1.15.m3.1.13.1.cmml" id="S3.p1.15.m3.1.13.1">p</mi></msup></mrow></mrow><annotation-xml xref="S3.p1.15.m3.1" id="S3.p1.15.m3.1.cmml" encoding="MathML-Content"><apply xref="S3.p1.15.m3.1.14" id="S3.p1.15.m3.1.14.cmml"><geq xref="S3.p1.15.m3.1.9" id="S3.p1.15.m3.1.9.cmml"></geq><apply xref="S3.p1.15.m3.1.14.1" id="S3.p1.15.m3.1.14.1.cmml"><times xref="S3.p1.15.m3.1.14.1.1" id="S3.p1.15.m3.1.14.1.1.cmml"></times><ci xref="S3.p1.15.m3.1.1" id="S3.p1.15.m3.1.1.cmml">B</ci><apply xref="S3.p1.15.m3.1.14.1.2" id="S3.p1.15.m3.1