<p class="ltx_p" id="S3.p1.8">therefore for every <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.8.m1.1.cmml" alttext="E\subset[0,1-1/\lambda^{p}]" class="ltx_Math" id="S3.p1.8.m1.1" display="inline"><semantics xref="S3.p1.8.m1.1.cmml" id="S3.p1.8.m1.1a"><mrow xref="S3.p1.8.m1.1.13.cmml" id="S3.p1.8.m1.1.13"><mi xref="S3.p1.8.m1.1.1.cmml" id="S3.p1.8.m1.1.1">E</mi><mo xref="S3.p1.8.m1.1.2.cmml" id="S3.p1.8.m1.1.2">‚äÇ</mo><mrow xref="S3.p1.8.m1.1.13.1.cmml" id="S3.p1.8.m1.1.13.1"><mo xref="S3.p1.8.m1.1.13.1.cmml" id="S3.p1.8.m1.1.13.1a">[</mo><mrow xref="S3.p1.8.m1.1.13.1.cmml" id="S3.p1.8.m1.1.13.1b"><mn xref="S3.p1.8.m1.1.4.cmml" id="S3.p1.8.m1.1.4">0</mn><mo xref="S3.p1.8.m1.1.13.1.cmml" id="S3.p1.8.m1.1.13.1c">,</mo><mrow xref="S3.p1.8.m1.1.13.1.2.cmml" id="S3.p1.8.m1.1.13.1.2"><mn xref="S3.p1.8.m1.1.6.cmml" id="S3.p1.8.m1.1.6">1</mn><mo xref="S3.p1.8.m1.1.7.cmml" id="S3.p1.8.m1.1.7">-</mo><mrow xref="S3.p1.8.m1.1.13.1.2.1.cmml" id="S3.p1.8.m1.1.13.1.2.1"><mn xref="S3.p1.8.m1.1.8.cmml" id="S3.p1.8.m1.1.8">1</mn><mo xref="S3.p1.8.m1.1.9.cmml" id="S3.p1.8.m1.1.9">/</mo><msup xref="S3.p1.8.m1.1.13.1.2.1.1.cmml" id="S3.p1.8.m1.1.13.1.2.1.1"><mi xref="S3.p1.8.m1.1.10.cmml" id="S3.p1.8.m1.1.10">Œª</mi><mi xref="S3.p1.8.m1.1.11.1.cmml" id="S3.p1.8.m1.1.11.1">p</mi></msup></mrow></mrow></mrow><mo xref="S3.p1.8.m1.1.13.1.cmml" id="S3.p1.8.m1.1.13.1d">]</mo></mrow></mrow><annotation-xml xref="S3.p1.8.m1.1" id="S3.p1.8.m1.1.cmml" encoding="MathML-Content"><apply xref="S3.p1.8.m1.1.13" id="S3.p1.8.m1.1.13.cmml"><subset xref="S3.p1.8.m1.1.2" id="S3.p1.8.m1.1.2.cmml"></subset><ci xref="S3.p1.8.m1.1.1" id="S3.p1.8.m1.1.1.cmml">E</ci><apply xref="S3.p1.8.m1.1.13.1" id="S3.p1.8.m1.1.13.1.cmml"><interval id="S3.p1.8.m1.1.13.1.1.cmml" closure="closed"></interval><cn xref="S3.p1.8.m1.1.4" id="S3.p1.8.m1.1.4.cmml" type="integer">0</cn><apply xref="S3.p1.8.m1.1.13.1.2" id="S3.p1.8.m1.1.13.1.2.cmml"><minus xref="S3.p1.8.m1.1.7" id="S3.p1.8.m1.1.7.cmml"></minus><cn xref="