<br class="ltx_break"></br>Since <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.p1.16.m3.1.cmml" alttext="\int_{0}^{1-1/\lambda^{p}}(1-\frac{1}{p})(s+1/\lambda^{p})^{-1/p}ds=1-1/%&#10;\lambda^{p-1}\geq f-1/\lambda^{p-1}" class="ltx_Math" id="S3.p1.16.m3.1" display="inline"><semantics xref="S3.p1.16.m3.1.cmml" id="S3.p1.16.m3.1a"><mrow xref="S3.p1.16.m3.1.34.cmml" id="S3.p1.16.m3.1.34"><mrow xref="S3.p1.16.m3.1.34.2.cmml" id="S3.p1.16.m3.1.34.2"><msubsup xref="S3.p1.16.m3.1.34.2.1.cmml" id="S3.p1.16.m3.1.34.2.1"><mo xref="S3.p1.16.m3.1.1.cmml" id="S3.p1.16.m3.1.1">‚à´</mo><mn xref="S3.p1.16.m3.1.2.1.cmml" id="S3.p1.16.m3.1.2.1">0</mn><mrow xref="S3.p1.16.m3.1.3.1.cmml" id="S3.p1.16.m3.1.3.1"><mn xref="S3.p1.16.m3.1.3.1.1.cmml" id="S3.p1.16.m3.1.3.1.1">1</mn><mo xref="S3.p1.16.m3.1.3.1.2.cmml" id="S3.p1.16.m3.1.3.1.2">-</mo><mrow xref="S3.p1.16.m3.1.3.1.7.cmml" id="S3.p1.16.m3.1.3.1.7"><mn xref="S3.p1.16.m3.1.3.1.3.cmml" id="S3.p1.16.m3.1.3.1.3">1</mn><mo xref="S3.p1.16.m3.1.3.1.4.cmml" id="S3.p1.16.m3.1.3.1.4">/</mo><msup xref="S3.p1.16.m3.1.3.1.7.1.cmml" id="S3.p1.16.m3.1.3.1.7.1"><mi xref="S3.p1.16.m3.1.3.1.5.cmml" id="S3.p1.16.m3.1.3.1.5">Œª</mi><mi xref="S3.p1.16.m3.1.3.1.6.1.cmml" id="S3.p1.16.m3.1.3.1.6.1">p</mi></msup></mrow></mrow></msubsup><mrow xref="S3.p1.16.m3.1.34.2.2.cmml" id="S3.p1.16.m3.1.34.2.2"><mrow xref="S3.p1.16.m3.1.34.2.2.2.cmml" id="S3.p1.16.m3.1.34.2.2.2"><mo xref="S3.p1.16.m3.1.34.2.2.2.cmml" id="S3.p1.16.m3.1.34.2.2.2a">(</mo><mrow xref="S3.p1.16.m3.1.34.2.2.2.cmml" id="S3.p1.16.m3.1.34.2.2.2b"><mn xref="S3.p1.16.m3.1.5.cmml" id="S3.p1.16.m3.1.5">1</mn><mo xref="S3.p1.16.m3.1.6.cmml" id="S3.p1.16.m3.1.6">-</mo><mfrac xref="S3.p1.16.m3.1.7.cmml" id="S3.p1.16.m3.1.7"><mn xref="S3.p1.16.m3.1.7.2.cmml" id="S3.p1.16.m3.1.7.2">1</mn><mi xref="S3.p1.16.m3.1.7.3.cmml" id="S3.p1.16.m3.1.7.3">p</mi></mfrac></mrow><mo xref="S3.p1.16.m3.1.34.2.2.2.cmml" id="S3.p1.16.m3.1.34.2.2.2c">)</mo></mrow><mo xref="S3.p1.16.m3.1.34.2.2.1.cmml" id="S3.p