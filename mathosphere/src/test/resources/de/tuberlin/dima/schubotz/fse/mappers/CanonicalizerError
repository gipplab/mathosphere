<math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.SS3.p8.3.m3.1.cmml" alttext="z&gt;z_{0}" class="ltx_Math" id="S3.SS3.p8.3.m3.1" display="inline"><semantics xref="S3.SS3.p8.3.m3.1.cmml" id="S3.SS3.p8.3.m3.1a"><mrow xref="S3.SS3.p8.3.m3.1.5.cmml" id="S3.SS3.p8.3.m3.1.5"><mi xref="S3.SS3.p8.3.m3.1.1.cmml" id="S3.SS3.p8.3.m3.1.1">z</mi><mo xref="S3.SS3.p8.3.m3.1.2.cmml" id="S3.SS3.p8.3.m3.1.2">&gt;</mo><msub xref="S3.SS3.p8.3.m3.1.5.1.cmml" id="S3.SS3.p8.3.m3.1.5.1"><mi xref="S3.SS3.p8.3.m3.1.3.cmml" id="S3.SS3.p8.3.m3.1.3">z</mi><mn xref="S3.SS3.p8.3.m3.1.4.1.cmml" id="S3.SS3.p8.3.m3.1.4.1">0</mn></msub></mrow><annotation-xml xref="S3.SS3.p8.3.m3.1" id="S3.SS3.p8.3.m3.1.cmml" encoding="MathML-Content"><apply xref="S3.SS3.p8.3.m3.1.5" id="S3.SS3.p8.3.m3.1.5.cmml"><gt xref="S3.SS3.p8.3.m3.1.2" id="S3.SS3.p8.3.m3.1.2.cmml"></gt><ci xref="S3.SS3.p8.3.m3.1.1" id="S3.SS3.p8.3.m3.1.1.cmml">z</ci><apply xref="S3.SS3.p8.3.m3.1.5.1" id="S3.SS3.p8.3.m3.1.5.1.cmml"><csymbol cd="ambiguous" id="S3.SS3.p8.3.m3.1.5.1.1.cmml">subscript</csymbol><ci xref="S3.SS3.p8.3.m3.1.3" id="S3.SS3.p8.3.m3.1.3.cmml">z</ci><cn xref="S3.SS3.p8.3.m3.1.4.1" id="S3.SS3.p8.3.m3.1.4.1.cmml" type="integer">0</cn></apply></apply></annotation-xml><annotation xref="S3.SS3.p8.3.m3.1.cmml" id="S3.SS3.p8.3.m3.1b" encoding="application/x-tex">z&gt;z_{0}</annotation></semantics></math> (<math xmlns="http://www.w3.org/1998/Math/MathML" xref="S3.SS3.p8.3.m4.1.cmml" alttext="X&gt;1" class="ltx_Math" id="S3.SS3.p8.3.m4.1" display="inline"><semantics xref="S3.SS3.p8.3.m4.1.cmml" id="S3.SS3.p8.3.m4.1a"><mrow xref="S3.SS3.p8.3.m4.1.4.cmml" id="S3.SS3.p8.3.m4.1.4"><mi xref="S3.SS3.p8.3.m4.1.1.cmml" id="S3.SS3.p8.3.m4.1.1">X</mi><mo xref="S3.SS3.p8.3.m4.1.2.cmml" id="S3.SS3.p8.3.m4.1.2">&gt;</mo><mn xref="S3.SS3.p8.3.m4.1.3.cmml" id="S3.SS3.p8.3.m4.1.3">1</mn></mrow><annotation-xml xref="S3.SS3.p8.3.m4.1" id="S3.SS3.p8.3.m4.1.cmml" encoding="MathML-Content"><apply xref="S3.SS3.p8.3.m4.1.4" id="S3.SS3.p8.3.m4.1.4.c