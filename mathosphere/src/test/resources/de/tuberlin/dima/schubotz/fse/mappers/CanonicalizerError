For fixed <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S4.p9.2.m8.1.cmml" alttext="l" class="ltx_Math" id="S4.p9.2.m8.1" display="inline"><semantics xref="S4.p9.2.m8.1.cmml" id="S4.p9.2.m8.1a"><mi xref="S4.p9.2.m8.1.1.cmml" id="S4.p9.2.m8.1.1">l</mi><annotation-xml xref="S4.p9.2.m8.1" id="S4.p9.2.m8.1.cmml" encoding="MathML-Content"><ci xref="S4.p9.2.m8.1.1" id="S4.p9.2.m8.1.1.cmml">l</ci></annotation-xml><annotation xref="S4.p9.2.m8.1.cmml" id="S4.p9.2.m8.1b" encoding="application/x-tex">l</annotation></semantics></math> the function <math xmlns="http://www.w3.org/1998/Math/MathML" xref="S4.p9.2.m9.1.cmml" alttext="p_{1}\longmapsto f^{(l)}_{p_{1}}" class="ltx_Math" id="S4.p9.2.m9.1" display="inline"><semantics xref="S4.p9.2.m9.1.cmml" id="S4.p9.2.m9.1a"><mrow xref="S4.p9.2.m9.1.7.cmml" id="S4.p9.2.m9.1.7"><msub xref="S4.p9.2.m9.1.7.1.cmml" id="S4.p9.2.m9.1.7.1"><mi xref="S4.p9.2.m9.1.1.cmml" id="S4.p9.2.m9.1.1">p</mi><mn xref="S4.p9.2.m9.1.2.1.cmml" id="S4.p9.2.m9.1.2.1">1</mn></msub><mo xref="S4.p9.2.m9.1.3.cmml" id="S4.p9.2.m9.1.3">‚üº</mo><msubsup xref="S4.p9.2.m9.1.7.2.cmml" id="S4.p9.2.m9.1.7.2"><mi xref="S4.p9.2.m9.1.4.cmml" id="S4.p9.2.m9.1.4">f</mi><msub xref="S4.p9.2.m9.1.6.1.cmml" id="S4.p9.2.m9.1.6.1"><mi xref="S4.p9.2.m9.1.6.1.1.cmml" id="S4.p9.2.m9.1.6.1.1">p</mi><mn xref="S4.p9.2.m9.1.6.1.2.1.cmml" id="S4.p9.2.m9.1.6.1.2.1">1</mn></msub><mrow xref="S4.p9.2.m9.1.5.1.cmml" id="S4.p9.2.m9.1.5.1"><mo xref="S4.p9.2.m9.1.5.1.cmml" id="S4.p9.2.m9.1.5.1a">(</mo><mi xref="S4.p9.2.m9.1.5.1.cmml" id="S4.p9.2.m9.1.5.1b">l</mi><mo xref="S4.p9.2.m9.1.5.1.cmml" id="S4.p9.2.m9.1.5.1c">)</mo></mrow></msubsup></mrow><annotation-xml xref="S4.p9.2.m9.1" id="S4.p9.2.m9.1.cmml" encoding="MathML-Content"><apply xref="S4.p9.2.m9.1.7" id="S4.p9.2.m9.1.7.cmml"><ci xref="S4.p9.2.m9.1.3" id="S4.p9.2.m9.1.3.cmml">normal-‚üº</ci><apply xref="S4.p9.2.m9.1.7.1" id="S4.p9.2.m9.1.7.1.cmml"><csymbol cd="ambiguous" id="S4.p9.2.m9.1.7.1.1.cmml">subscript</csymbol><ci xref="S4.p